{% for message in app.session.getFlashBag.get('success') %}
  <br>
  <br>
  <div class="container position-fixed top-505 start-505 translate-middle" style="width:300px;">
    <div class="alert alert-success text-center" role="alert" style="background-color: #28a745; border-color: #28a745; color: #fff; border-radius: .25rem;">
      {{ message }}
    </div>
  </div>

  <script>
    setTimeout(function() {
        document.querySelector('.alert-success').remove();
    }, 3000);
  </script>
{% endfor %}

